        app:layout_constraintEnd_toStartOf="@+id/gl_entire_ver_5"
        app:layout_constraintStart_toStartOf="@+id/gl_entire_ver_4"
        app:layout_constraintTop_toTopOf="@+id/gl_entire_hor_10"
